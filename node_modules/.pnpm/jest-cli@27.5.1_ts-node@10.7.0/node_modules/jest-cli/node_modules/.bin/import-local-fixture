#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jest-cli@27.5.1_ts-node@10.7.0/node_modules/jest-cli/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jest-cli@27.5.1_ts-node@10.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jest-cli@27.5.1_ts-node@10.7.0/node_modules/jest-cli/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jest-cli@27.5.1_ts-node@10.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../../../../import-local@3.1.0/node_modules/import-local/fixtures/cli.js" "$@"
fi
