#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/browserslist@4.20.2/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/core-js-compat@3.21.1/node_modules/core-js-compat/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/core-js-compat@3.21.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/browserslist@4.20.2/node_modules/browserslist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/browserslist@4.20.2/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/core-js-compat@3.21.1/node_modules/core-js-compat/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/core-js-compat@3.21.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.20.2/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.20.2/node_modules/browserslist/cli.js" "$@"
fi
