#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules/@preconstruct/cli/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules/@preconstruct/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules/npm-packlist/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/npm-packlist@2.2.2/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules/@preconstruct/cli/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules/@preconstruct/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/@preconstruct+cli@2.1.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../npm-packlist@2.2.2/node_modules/npm-packlist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../npm-packlist@2.2.2/node_modules/npm-packlist/bin/index.js" "$@"
fi
