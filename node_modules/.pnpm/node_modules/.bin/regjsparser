#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules/regjsparser/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules/regjsparser/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules/regjsparser/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/regjsparser@0.8.4/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules/regjsparser/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules/regjsparser/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../regjsparser/bin/parser" "$@"
fi
