#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jsdom@16.7.0/node_modules/jsdom/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jsdom@16.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/bin/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/escodegen@2.0.0/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules:/Users/rfarrer/projects/node_modules:/Users/rfarrer/node_modules:/Users/node_modules:/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jsdom@16.7.0/node_modules/jsdom/node_modules:/Users/rfarrer/projects/rainbow-sprinkles/node_modules/.pnpm/jsdom@16.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../../../../../escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
fi
